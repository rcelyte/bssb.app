<!doctype html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
<meta name="description" content="Beat Saber modification that adds a browser for custom multiplayer games.">
<title>{% if pageTitle %}{{ pageTitle }} - {% endif %}Beat Saber Server Browser Â· BSSB</title>
<link rel="canonical" href="https://bssb.app{{ pageUrl }}">
<link href="/static/vendor/bootstrap.min.css" rel="stylesheet">
<link href="/static/bssb.css?v={{ version_hash_short }}" rel="stylesheet">
<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
<link rel="manifest" href="/site.webmanifest">
<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
<meta name="msapplication-TileColor" content="#603cba">
<meta name="theme-color" content="#9f00a7">
{% if ldJson is defined %}
<script type="application/ld+json">
{{ ldJson|json_encode(constant('JSON_PRETTY_PRINT'))|raw }}
</script>
{% endif %}
</head>
<body>

<!-- Page generated {{ "now"|date('r') }} -->

{% include "navbar.twig" %}

{% block main %}{% endblock %}

{% include "footer.twig" %}

<script src="/static/vendor/darkmode.js"></script>

<link href="/static/vendor/materialdesignicons.min.css" rel="stylesheet">

<script src="/static/vendor/jquery-3.5.1.slim.min.js"></script>
<script src="/static/vendor/bootstrap.bundle.min.js"></script>

<script>$(function () {
  $('[data-toggle="tooltip"]').tooltip();
  $('.filters > input, select').change(() => {
    $('#filters').submit();
  });
})</script>

{% block scripts %}{% endblock %}

</body>
</html>